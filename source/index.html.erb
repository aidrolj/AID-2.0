              <label>Checka</label>
              <input id="checkbox1" type="checkbox"><label for="checkbox1">1</label>
              <input id="checkbox2" type="checkbox"><label for="checkbox2">2</label>
            </div>
          </div>
          <div class="row">
            <div class="large-12 columns">
              <label>Ärende
                <textarea rows="4" placeholder="Ditt ärende..."></textarea>
              </label>
            </div>
          </div>
          <div class="row">
            <div class="small-6 small-centered columns">
              <br/>
              <a href="#" class="button radius alert">Skicka</a>
            </div>
          </div>

        </div>
        <!-- drift-support form end -->
      </div>
      <!-- drift-support image/form row end -->

      <!-- drift-support text -->
      <div class="row main-content-text">
        <div class="large-12 columns">

            <h2 class="inline"><%= sub_03.title %>.</h2>
            <p><%= sub_03.text %> <a href="<%= sub_03.url %>"><span class="label secondary">Läs mer &raquo;</span></a></p>
        </div>
      </div>
      <!-- drift-support text end -->

    </section>
    <!-- drift-support section end -->


    <!-- webb-utveckling section -->
    <section>
      <div class="row border" id="natverk" data-equalizer>

        <!-- webb-utveckling image -->
        <div class="medium-8 columns" data-equalizer-watch>
          <%= image_tag "aid-photo09.jpg" %>
        </div>
        <!-- webb-utveckling image end -->

        <!-- webb-utveckling form -->
        <div class="medium-4 columns panel" data-equalizer-watch>
          <div class="row">
            <div class="large-12 columns">
