            </div>
            <div class="large-6 columns">
              <label>Checka</label>
              <input id="checkbox1" type="checkbox"><label for="checkbox1">1</label>
              <input id="checkbox2" type="checkbox"><label for="checkbox2">2</label>
            </div>
          </div>
          <div class="row">
            <div class="large-12 columns">
              <label>Ärende
                <textarea rows="4" placeholder="Ditt ärende..."></textarea>
              </label>
            </div>
          </div>
          <div class="row">
            <div class="small-6 small-centered columns">
              <br/>
              <a href="#" class="button radius alert">Skicka</a>
            </div>
          </div>

        </div>
        <!-- webb-utveckling form end -->
      </div>
      <!-- webb-utveckling image/form row end -->

      <!-- webb-utveckling text -->
      <div class="row main-content-text">
        <div class="large-12 columns">

            <h2 class="inline"><%= sub_04.title %>.</h2>
            <p><%= sub_04.text %> <a href="<%= sub_04.url %>"><span class="label secondary">Läs mer &raquo;</span></a></p>
        </div>
      </div>
      <!-- webb-utveckling text end -->

    </section>
    <!-- webb-utveckling section end -->

  </div>
  <!-- main content end -->
</section>
<!-- main section end -->
