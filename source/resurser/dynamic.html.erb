---
title: Test
---

<div class="resource-content">

  <div class="row">
    <div class="small-12 columns">
      <h3>Dokument</h3>

        <table class="responsive" width="100%">
          <thead>
            <tr>
              <th width="20%">Titel</th>
              <th width="55%">Beskrivning</th>
              <th width="10%">Datum</th>
              <th width="10%">Storlek</th>
              <th width="5%"></th>
            </tr>
          </thead>
          <tbody>

          <% data.resurser.dokument.info.each do |r| %>
            <tr>
              <td><a href="/assets/files/<%=r.url%>"><%= r.title %></a></td>
              <td><%= r.description %></td>
              <td><%= r.date %></td>
              <td><%= r.size %></td>
              <td><center><a href="/assets/files/<%=r.url%>"><i class="fi-download"></i></a></center></td>
            </tr>
          <% end %>

          </tbody>

        </table>
    </div>
  </div>

  <div class="row resources-nav">
    <div class="small-6 columns"><a href="/resurser/applikationer"><i class="fi-arrow-left"></i> <small>Applikationer</small></a></div>
    <div class="small-6 columns"><a href="/resurser/manualer"> <i class="fi-arrow-right right"> </i><span class="right"><small>Manualer</small>&nbsp;</span></a></div>
  </div>

</div>
